<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net7.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
	</PropertyGroup>

	<ItemGroup>
		<None Include="..\..\LICENSE">
			<Pack>True</Pack>
			<PackagePath>\</PackagePath>
		</None>
		<None Include="..\..\icon.png">
			<Pack>True</Pack>
			<PackagePath>\</PackagePath>
		</None>
		<None Include="README_NUGET.md">
			<Pack>True</Pack>
			<PackagePath>\</PackagePath>
		</None>
	</ItemGroup>


	<ItemGroup Condition="'$(Configuration)' == 'Debug'">
		<ProjectReference Include="..\Genocs.Core\Genocs.Core.csproj" />
		<ProjectReference Include="..\Genocs.WebApi\Genocs.WebApi.csproj" />

	</ItemGroup>

	<ItemGroup Condition="'$(Configuration)' == 'Release'">
		<ProjectReference Include="Genocs.WebApi" Version="5.0.*" />
		<ProjectReference Include="Genocs.Core" Version="5.0.*" />
	</ItemGroup>

</Project>
